\"react-router-dom\";\nimport Home from './pages/home';\nimport RecipesList from './pages/recipes/list';\nimport Recipe from './pages/recipes/recipe';\nimport Header from './pages/header/index.js';\nimport NewRecipe from './pages/recipes/newRecipe';\nimport EditRecipe from './pages/recipes/edit';\n\nfunction App() {\n  return (\n   <Router>\n     <Header />\n     <Switch>\n     <Route path=\"/recipes/:id/edit\">\n        <EditRecipe /> \n      </Route>\n     <Route path=\"/recipes/create\">\n         <NewRecipe />\n      </Route>\n      <Route path=\"/recipes/:id\">\n         <Recipe />\n      </Route>\n      <Route path=\"/recipes\" exact>\n         <RecipesList />\n         \n      </Route>\n      <Route path=\"/\">\n        <Home/>\n      </Route>\n\n     </Switch>\n\n   </Router>\n  );\n}\n\nexport default App;\n","/home/hector/Escritorio/Fullstack/node/frontend_recipes/recipes/src/pages/home/index.js",[],"/home/hector/Escritorio/Fullstack/node/frontend_recipes/recipes/src/pages/recipes/list/index.js",["53","54","55","56"],"import {useEffect, useState} from 'react';\nimport {Link, useHistory, useLocation} from 'react-router-dom';\nimport { deleteRecipe, useFilterRecipes } from '../../../api';\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome'\nimport {faTrashAlt} from '@fortawesome/free-solid-svg-icons'\n\n\n\nconst RecipesItems = (props) => {\n\n    const deleteRecipeHandler = deleteRecipe(props.id);\n    const history = useHistory();\n    \n    const handleRemove = () => {\n        deleteRecipeHandler().then(response => history.go(0));\n     }\n    \n \n    return (<div>\n        <span>{props.title}</span>\n        <button onClick={handleRemove}><FontAwesomeIcon icon={faTrashAlt} /></button> \n        <Link to={'/recipes/'+props.id}>\n            <button>\n                Recipe\n            </button>\n        </Link>\n       \n    </div>)\n    \n}\nconst parseQuery = (queryString) => {\n    var search = queryString.substring(1);\n    JSON.parse('{\"' + search.replace(/&/g, '\",\"').replace(/=/g,'\":\"') + '\"}', function(key, value) { return key===\"\"?value:decodeURIComponent(value) })\n}\n\n\n\nexport default function RecipesList () {\n    \n         \n    const history = useHistory();\n    const location = useLocation();\n    \n    //convert to an object\n    var index = location.search.indexOf(\"=\");\n    var query = location.search.substring(index+1, location.search.length);\n    let values = query.split(\",\");\n   \n\n    const recipes = useFilterRecipes(query);\n\n    \n  \n    const searchBarKeyword = e => {\n        if(e.key === 'Enter'){\n            const search = e.target.value;\n            const url = '/recipes?keywords='+search;\n            if(search === \"\"){\n                return history.push('/recipes');\n            }\n            return history.push(url);   \n          \n            \n        }\n    }\n    const searchBarTitle = e => {\n        if(e.key === 'Enter'){\n            const search = e.target.value;\n            const url = '/recipes?title='+search;\n            if(search === \"\"){\n                return history.push('/recipes');\n            }\n            return history.push(url);   \n          \n                        \n        }\n        \n    }\n   \n   \n    return (\n        <div>\n            <div>\n                <div><span>Title Search Bar:  <input type=\"text\" onKeyPress={searchBarTitle}/></span></div>\n                <div><span>Keyword Search Bar:  <input type=\"text\" onKeyPress={searchBarKeyword}/></span></div>\n                <br/>\n            </div>\n            Recipes: {query || \"\"}\n            <div>\n                <Link to={'/recipes/create'}>\n                <button>\n                    Create\n                </button>\n                </Link>\n            </div>\n            \n            {recipes.map(r => <RecipesItems {...r}/>)}\n            <Link to=\"/\">\n                <button>Home</button>\n            </Link>\n        </div>\n    )\n    \n}\n","/home/hector/Escritorio/Fullstack/node/frontend_recipes/recipes/src/pages/recipes/recipe/index.js",["57"],"import {useParams, Link,useHistory} from \"react-router-dom\";\nimport {UseRecipe, deleteRecipe} from '../../../api';\nimport './recipe.css';\n\n\n\nconst GoBack = () => {\n    const history = useHistory(); \n    return(\n       <button onClick={history.goBack}>Back!</button>\n    )\n}\n\n\nexport default function Recipe (){\n    \n    const { id } = useParams();\n\n    const recipe = UseRecipe(id);\n    const history = useHistory();\n    const deleteRecipeHandler = deleteRecipe(id);\n    \n     if(!recipe){\n         return <h2>Loading the recipe...wait please</h2>\n     }\n     const handleRemove = () => {\n        deleteRecipeHandler().then(response => history.push('/recipes'));\n     }\n    const url = `/recipes/${recipe.id}/edit`\n    return(\n        <div>\n            <div>\n            <h3>Recipe: {recipe.title}</h3>\n            <Link to={url}>\n                <button>Edit recipe</button>\n            </Link>  \n            <h4>Keywords: {recipe.keywords.map(keyword => <span>\"{keyword}\" </span>)}</h4>\n                    \n            <img className=\"photo\" src={recipe.photo} />\n            </div>\n            \n           <GoBack />\n           <Link to=\"/\">\n                <button>Home</button>\n            </Link>\n            <button onClick={handleRemove}> Delete </button>\n        </div>\n    )\n}","/home/hector/Escritorio/Fullstack/node/frontend_recipes/recipes/src/api/index.js",["58"],"/home/hector/Escritorio/Fullstack/node/frontend_recipes/recipes/src/pages/header/index.js",["59"],"import './header.css'\nimport {useState} from 'react';\n\nexport default function Header (){\n    \n   \n    return (\n        <div className=\"header\">\n           \n            Welcome to our Recipe App!\n           \n         </div>\n         \n    )\n}","/home/hector/Escritorio/Fullstack/node/frontend_recipes/recipes/src/pages/recipes/newRecipe/index.js",["60","61","62"],"import {useParams, Link,useHistory} from \"react-router-dom\";\nimport {UseRecipe, deleteRecipe, createRecipe} from '../../../api';\nimport {useRef, useState} from 'react';\nimport './newRecipe.css';\n\n\nconst NewRecipe = () => {\n    const [waiting, setWaiting] = useState(false);\n    const history = useHistory();\n    const createNewRecipe = createRecipe();\n    const titleRef = useRef();\n    const keywordsRef = useRef();\n\n    const handleCreate = () => {\n        setWaiting(true);\n        createNewRecipe({\n            title: titleRef.current.value,\n            keywords: keywordsRef.current.value.split(\",\"),\n        }).then(response => history.goBack(-2))\n    }\n    \n    return(\n        <div>\n            <div>\n                <h3>New Recipe</h3>\n                <input type=\"text\" placeholder=\"Title\" ref={titleRef}/>\n                <input type=\"text\" placeholder=\"Keywords\" ref={keywordsRef} />\n                <button onClick={handleCreate} disabled={waiting}> \n                    {waiting ? \"Please wait\": \"Create\"} \n                    </button>\n            \n            </div>\n            \n            <Link to=\"/recipes\">\n                <button>Back</button>\n            </Link>\n           <Link to=\"/\">\n                <button>Home</button>\n            </Link>\n           \n        </div>\n    )\n}\nexport default NewRecipe;","/home/hector/Escritorio/Fullstack/node/frontend_recipes/recipes/src/pages/recipes/edit/index.js",[],{"ruleId":"63","severity":1,"message":"64","line":5,"column":8,"nodeType":"65","messageId":"66","endLine":5,"endColumn":23},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","replacedBy":"70"},{"ruleId":"63","severity":1,"message":"71","line":6,"column":3,"nodeType":"65","messageId":"66","endLine":6,"endColumn":7},{"ruleId":"63","severity":1,"message":"72","line":1,"column":9,"nodeType":"65","messageId":"66","endLine":1,"endColumn":18},{"ruleId":"63","sever